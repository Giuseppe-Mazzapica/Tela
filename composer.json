{
    "name": "giuseppe-mazzapica/tela",
    "description": "PHP/jQuery tool for super-easy ajax in WordPress",
    "keywords": ["wordpress"],
    "homepage": "https://github.com/Giuseppe-Mazzapica/Tela",
    "license": "MIT",
    "authors": [
        {
            "name": "Giuseppe Mazzapica",
            "email": "giuseppe.mazzapica@gmail.com",
            "homepage": "http://gm.zoomlab.it",
            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/Giuseppe-Mazzapica/Tela/issues",
        "source": "https://github.com/Giuseppe-Mazzapica/Tela"
    },
    "require": {
        "php": ">=5.4"
    },
    "require-dev": {
        "phpunit/phpunit": "3.7.*",
        "10up/wp_mock": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "GM\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GM\\Tela\\Tests\\": "tests/src/"
        }
    },
    "config": {
        "optimize-autoloader": true
    }
}
